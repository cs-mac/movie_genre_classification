Dialogue: 0,0:03:55.65,0:04:00.78,Default,,0,0,0,,# {\i1}Never met{\i0} a {\i1}wise man{\i0}
Dialogue: 0,0:04:00.90,0:04:05.87,Default,,0,0,0,,{\i1}# If so, it's a woman{\i0}
